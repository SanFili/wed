{
  "name": "intelsy-frontend",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "export": "next build && next export",
    "start": "next start",
    "check": "yarn run lint:es && yarn run lint:style && yarn run type-check",
    "lint:es": "next lint",
    "lint:es:fix": "next lint --fix",
    "lint:style": "stylelint \"**/*.scss\" --max-warnings 0",
    "lint:style:fix": "stylelint --fix \"**/*.scss\"",
    "type-check": "tsc --noEmit"
  },
  "lint-staged": {
    "*.{ts,tsx}": [
      "eslint --ignore-path .gitignore --max-warnings 0"
    ],
    "*.{scss,css}": [
      "stylelint --ignore-path .gitignore --max-warnings 0"
    ]
  },
  "dependencies": {
    "@apollo/client": "^3.5.10",
    "@types/qs": "^6.9.7",
    "classnames": "^2.3.1",
    "next": "12.1.4",
    "react": "18.0.0",
    "react-dom": "18.0.0",
    "react-intersection-observer": "^8.33.1",
    "react-transition-group": "^4.4.5",
    "reset-css": "^5.0.1",
    "swiper": "^8.0.7"
  },
  "devDependencies": {
    "@types/node": "17.0.23",
    "@types/react": "17.0.43",
    "@types/react-transition-group": "^4.4.4",
    "@typescript-eslint/eslint-plugin": "^5.17.0",
    "@typescript-eslint/parser": "^5.17.0",
    "dotenv": "^16.0.3",
    "eslint": "8.12.0",
    "eslint-config-airbnb-typescript": "^16.1.4",
    "eslint-config-next": "12.1.4",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-promise": "^6.0.0",
    "eslint-plugin-simple-import-sort": "^7.0.0",
    "lint-staged": "^12.3.7",
    "prettier": "^2.6.1",
    "sass": "^1.49.10",
    "stylelint": "^14.6.1",
    "stylelint-config-recommended": "^7.0.0",
    "stylelint-config-standard": "^25.0.0",
    "stylelint-config-standard-scss": "^3.0.0",
    "stylelint-declaration-use-variable": "^1.7.3",
    "stylelint-order": "^5.0.0",
    "stylelint-scss": "^4.2.0",
    "typescript": "4.6.3"
  },
  "resolutions": {
    "@types/react": "17.0.2",
    "@types/react-dom": "17.0.2"
  }
}
